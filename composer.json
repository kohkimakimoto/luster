{
    "name": "kohkimakimoto/luster",
    "description": "A command line application framework based on Laravel.",
    "keywords": ["cli"],
    "license": "MIT",
    "homepage": "https://github.com/kohkimakimoto/luster",
    "require": {
        "php": ">=5.4.0",
        "illuminate/contracts": "5.0.*",
        "illuminate/database": "5.0.*",
        "illuminate/container": "5.0.*",
        "illuminate/console": "5.0.*",
        "illuminate/events": "5.0.*",
        "illuminate/view": "5.0.*",
        "illuminate/config": "5.0.*",
        "illuminate/support": "5.0.*",
        "illuminate/http": "5.0.*",
        "illuminate/filesystem": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "symfony/process": "2.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "fabpot/php-cs-fixer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Kohkimakimoto\\Luster\\": "src/"
        },
        "classmap": [
            "src/Foundation/Composer.php"
        ],
        "files": [
            "src/Foundation/helpers.php"
        ]
    },
    "bin": ["bin/luster"]
}
